{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "groww-buddy/blog-posts",
  "version": "1.0.0",
  "title": "Blog Posts",
  "description": "Display a list of blog posts.",
  "category": "growwbuddy",
  "icon": "admin-users",
  "keywords": [
    "posts",
    "groww-buddy",
    "blog-posts",
    "groww-buddy-blog-posts"
  ],
  "textdomain": "groww-buddy",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "example": {},
  "attributes": {
    "postsStyles": {
      "type": "object",
      "default": {
        "membersLayout": "list",
        "itemBgColor": "#ffffff",
        "itemBgHoverColor": "#f9f9f9"
      }
    },
    "postsSettings": {
      "type": "object",
      "default": {
        "filter": "filter_by",
        "search": "",
        "perPage": 10,
        "type": "newest"
      }
    }
  },
  "supports": {
    "align": true,
    "ariaLabel": true,
    "anchor": true,
    "html": false,
    "reusable": false,
    "spacing": {
      "padding": true,
      "margin": true
    },
    "dimensions": {
      "width": true,
      "height": true
    },
    "responsive": true,
    "typography": {
      "lineHeight": true
    },
    "color": {
      "gradients": true,
      "link": true
    },
    "background": {
      "color": true,
      "gradients": true
    }
  }
}
